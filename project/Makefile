# GNU Make workspace makefile autogenerated by Premake

.NOTPARALLEL:

ifndef config
  config=debug_x86
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug_x86)
  CAST_config = debug_x86
endif
ifeq ($(config),debug_x64)
  CAST_config = debug_x64
endif
ifeq ($(config),release_x86)
  CAST_config = release_x86
endif
ifeq ($(config),release_x64)
  CAST_config = release_x64
endif
ifeq ($(config),testing_x86)
  CAST_config = testing_x86
endif
ifeq ($(config),testing_x64)
  CAST_config = testing_x64
endif

PROJECTS := CAST

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

CAST:
ifneq (,$(CAST_config))
	@echo "==== Building CAST ($(CAST_config)) ===="
	@${MAKE} --no-print-directory -C . -f CAST.make config=$(CAST_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f CAST.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug_x86"
	@echo "  debug_x64"
	@echo "  release_x86"
	@echo "  release_x64"
	@echo "  testing_x86"
	@echo "  testing_x64"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   CAST"
	@echo ""
	@echo "For more information, see http://industriousone.com/premake/quick-start"